#include <iostream>
using namespace std;

int factorize(int A[], int n, int i = 2, int j = 0);

int main() {
    int n = 144;
    int A[100];
    int m = factorize(A, n);

    // Print the factors
    cout << n << " = ";
    for (int i = 0; i < m; i++) {
        if (i < m - 1)
            cout << A[i] << "x";
        else
            cout << A[i];
    }
    cout << endl;

    return 0;
}
